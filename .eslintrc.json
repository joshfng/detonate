{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "quotes": [2, "double", { "avoidEscape": true }],
    "comma-dangle": "off"
  }
}
